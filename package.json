{
  "name": "nodepi",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ex3ndr/nodepi.git",
  "author": "Steve Korshakov <steve@korshakov.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc && cpx ./src/packages/registry/Schema.graphql ./build/registry/ && cpx ./src/packages/launcher/index.js ./build/launcher/",
    "start": "yarn build && STORAGE_PATH=`pwd`/data node ./build/launcher/index.js",
    "postinstall": "tsc && cpx ./src/packages/registry/Schema.graphql ./build/registry/ && cpx ./src/packages/launcher/index.js ./build/launcher/"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.5",
    "cpx": "^1.5.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.5",
    "graphql-tools": "^4.0.5",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  }
}
